<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="lists" >
	<typeAlias alias="lists" type="com.alibaba.orland.dataobject.Lists" />
	<resultMap id="lists_BaseResultMap" class="lists" >
		<result column="name" property="id" jdbcType="VARCHAR" />
		<result column="category" property="category" jdbcType="VARCHAR" />
		<result column="content" property="content" jdbcType="VARCHAR" />
		<result column="action" property="action" jdbcType="INTEGER" />
		<result column="role_scope" property="roleScope" jdbcType="VARCHAR" />
		<result column="app_scope" property="appScope" jdbcType="VARCHAR" />
		<result column="partner_scope" property="partnerScope" jdbcType="VARCHAR" />
		<result column="descs" property="descs" jdbcType="VARCHAR" />
	</resultMap>
	<insert id="insert" parameterClass="lists" >
		insert into lists (name, category, content, action, role_scope, app_scope, partner_scope, descs) values (#id:VARCHAR#, #category:VARCHAR#, #content:VARCHAR#, #action:INTEGER#, #roleScope:VARCHAR#, #appScope:VARCHAR#, #partnerScope:VARCHAR#, #descs:VARCHAR#)
	</insert>
	<insert id="insertSelective" parameterClass="lists" >
		insert into lists
		<dynamic prepend="(" >
			<isNotNull prepend="," property="id" > name </isNotNull>
			<isNotNull prepend="," property="category" > category </isNotNull>
			<isNotNull prepend="," property="content" > content </isNotNull>
			<isNotNull prepend="," property="action" > action </isNotNull>
			<isNotNull prepend="," property="roleScope" > role_scope </isNotNull>
			<isNotNull prepend="," property="appScope" > app_scope </isNotNull>
			<isNotNull prepend="," property="partnerScope" > partner_scope </isNotNull>
			<isNotNull prepend="," property="descs" > descs </isNotNull>
			)
		</dynamic>
		values
		<dynamic prepend="(" >
			<isNotNull prepend="," property="id" > #id:VARCHAR# </isNotNull>
			<isNotNull prepend="," property="category" > #category:VARCHAR# </isNotNull>
			<isNotNull prepend="," property="content" > #content:VARCHAR# </isNotNull>
			<isNotNull prepend="," property="action" > #action:INTEGER# </isNotNull>
			<isNotNull prepend="," property="roleScope" > #roleScope:VARCHAR# </isNotNull>
			<isNotNull prepend="," property="appScope" > #appScope:VARCHAR# </isNotNull>
			<isNotNull prepend="," property="partnerScope" > #partnerScope:VARCHAR# </isNotNull>
			<isNotNull prepend="," property="descs" > #descs:VARCHAR# </isNotNull>
			)
		</dynamic>
	</insert>

	<select id="queryById" parameterClass="String" resultMap="lists_BaseResultMap">
		select name, category, content, action, role_scope, app_scope, partner_scope, descs from lists where name=#value#	
	</select>

	<update id="updateById" parameterClass="lists">
		update lists set 
		<dynamic prepend=" " >
			<isNotNull prepend="," property="category" > category = #category:VARCHAR# </isNotNull>
			<isNotNull prepend="," property="content" > content = #content:VARCHAR# </isNotNull>
			<isNotNull prepend="," property="action" > action = #action:INTEGER# </isNotNull>
			<isNotNull prepend="," property="roleScope" > role_scope = #roleScope:VARCHAR# </isNotNull>
			<isNotNull prepend="," property="appScope" > app_scope = #appScope:VARCHAR# </isNotNull>
			<isNotNull prepend="," property="partnerScope" > partner_scope = #partnerScope:VARCHAR# </isNotNull>
			<isNotNull prepend="," property="descs" > descs = #descs:VARCHAR# </isNotNull>
		</dynamic>
		where name=#id#
	</update>

	<delete id="deleteById" parameterClass="String">
		delete from lists where name=#value#	
	</delete>

	<select id="getAll" resultMap="lists_BaseResultMap">
		select name, category, content, action, role_scope, app_scope, partner_scope, descs from lists
	</select>

	<select id="getAllWithLikeGroup" resultMap="lists_BaseResultMap" parameterClass="String">
		select name, category, content, action, role_scope, app_scope, partner_scope, descs from lists where content like #v#
	</select>
</sqlMap>