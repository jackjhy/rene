<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="records_deleted" >
	<typeAlias alias="records_deleted" type="com.alibaba.orland.dataobject.DeletedRecords" />
	<resultMap id="records_deleted_BaseResultMap" class="records_deleted" >
		<result column="id" property="id" jdbcType="INTEGER" />
		<result column="list_id" property="listId" jdbcType="VARCHAR" />
		<result column="deleted_time" property="deletedTime" jdbcType="DATE" />
	</resultMap>
	<insert id="insert" parameterClass="records_deleted" >
		insert into records_deleted (id, list_id, deleted_time) values (#id:INTEGER#, #listId:INTEGER#, #deletedTime:DATE#)
	</insert>
	<insert id="insertSelective" parameterClass="records_deleted" >
		insert into records_deleted
		<dynamic prepend="(" >
			<isNotNull prepend="," property="id" > id </isNotNull>
			<isNotNull prepend="," property="listId" > list_id </isNotNull>
			<isNotNull prepend="," property="deletedTime" > deleted_time </isNotNull>
		)
		</dynamic>
		values
		<dynamic prepend="(" >
			<isNotNull prepend="," property="id" > #id:INTEGER# </isNotNull>
			<isNotNull prepend="," property="listId" > #listId:VARCHAR# </isNotNull>
			<isNotNull prepend="," property="deletedTime" > #deletedTime:DATE# </isNotNull>
		)
		</dynamic>
	</insert>

	<select id="queryByIdAndListsId" parameterClass="map" resultMap="records_deleted_BaseResultMap">
		select id,deleted_time,list_id from records_deleted where id=#id# and list_id=#listsid#
	</select>

	<delete id="deleteByIdAndListid" parameterClass="map">
		delete from records_deleted where id=#id# and list_id=#listsid#
	</delete>
</sqlMap>